print("Hello world!")       -- Tipe data: String (menggunakan tanda kutip ganda)
print('ini juga string')    -- Juga string (boleh menggunakan tanda kutip tunggal)

print(100)                  -- Tipe data: Number (Integer)
print(1.9)                  -- Tipe data: Number (Float)
print(-3)                   -- Tipe data: Number (Negatif)

print(true)                 -- Tipe data: Boolean (True)
print(false)                -- Tipe data: Boolean (False)

print(nil)                  -- Tipe data: Nil (kosong atau belum di-assign)

-- Catatan:
-- Luau bersifat dynamic typing, tidak butuh deklarasi tipe secara eksplisit.
-- Variabel yang belum di-assign nilainya adalah 'nil' secara default.
